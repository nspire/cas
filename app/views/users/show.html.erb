<%= link_to("Back to index", {:controller => 'access', :action => 'index'}, :class => "back-link") %>
<div class="users show">
	<h2>Admin Summary:</h2>
	<table summary="Users detail view">
		<tr>
			<th>Username</th>
			<th>Email</th>
			<th>Actions</th>
		</tr>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.username %></td>
				<td><%= user.email %></td>
				<td><%= link_to("Delete", {:action => 'delete', :id => user.id}) %></td>
			</tr>
		<% end %>
	</table>
</div>
