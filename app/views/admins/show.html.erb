<%= link_to("Back to index", {:action => 'index'}, :class => "back-link") %>
<div class="admins show">
	<h2>Admin Summary:</h2>
	<table summary="Admins detail view">
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>User Name</th>
			<th>Organization</th>
			<th>Email</th>
			<th>User Limit</th>
			<th>Created</th>
			<th>Updated</th>
		</tr>
		<tr>
			<td><%= @admin.fname %></td>
			<td><%= @admin.lname %></td>
			<td><%= @admin.username %></td>
			<td><%= @admin.organization %></td>
			<td><%= @admin.email %></td>
			<td><%= @admin.user_limit %></td>
			<td><%= @admin.created_at %></td>
			<td><%= @admin.updated_at %></td>
		</tr>
	</table>
</div>
